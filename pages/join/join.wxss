  margin-left: 20rpx;
  float: left;
  border-radius: 6rpx;
  color: #048176;
  font-size: 32rpx;
  /* border: 1px solid transparent; */
}
.blockBorder{
  /* border-color: #048176; */
  background-color: #048176;
  color: white;
}