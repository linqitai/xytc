.cate{
  padding: 20rpx 0rpx 20rpx 0;
  overflow: hidden;
}
.cate .cateItem{
  float: left;
  display: flex;
  flex-direction: column;
  width: 120rpx;
  height: 120rpx;
  padding: 0 15rpx;
  margin-top: 10rpx;
}
.cate .cateItem .picBox{
  text-align: center;
}
.cate .cateItem .pic{
  width: 70rpx;
  height: 70rpx;
  border-radius: 70rpx;
  background-color: forestgreen;
}
.cate .cateItem .name{
  width: 100%;
  font-size: 26rpx;
  text-align: center;
  color: #646464;
}