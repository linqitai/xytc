<view class="container m-b5">
  <view class="user-header p-r">
    <view class="user-bg-box">
      <!-- <image class="header-bg" src="{{userInfo.avatarUrl}}"></image> -->
       <open-data class="header-bg" type="userAvatarUrl"></open-data>
    </view>
  </view>
  <view>
    <view class="user-order">
      <view class="user-order-cont dis-flex">
        <view class="flex-box p-r">
          <navigator hover-class="navigator-hover" url="../order/index?type=all" open-type='navigate'>
            <text class="iconfont icon-form orderListIcon ">
            </text>
            <text class="listCont">全部订单</text>
          </navigator>
        </view>
        <view class="flex-box p-r">
          <navigator hover-class="navigator-hover" url="../order/index?type=delivery" open-type='navigate'>
            <text class="iconfont icon-daifukuan orderListIcon">
              <!-- <text class='tag' wx:if='{{orderCount.delivery>0}}'>{{orderCount.delivery}}</text> -->
            </text>
            <text class="listCont">待确认</text>
            <text class="user-tag p-a" wx:if="{{orderCount.delivery > 0}}">{{orderCount.delivery}}</text>
          </navigator>
        </view>
        <view class="p-r" style=" flex:1.4;">
          <view class="header-img-box">
             <!-- <image src="{{userInfo.avatarUrl}}"></image>  -->
             <open-data type="userAvatarUrl"></open-data> 
          </view>
          <!-- <text class="user-admin onelist-hidden">{{userInfo.nickName}}</text> -->
           <open-data class="user-admin onelist-hidden" type="userNickName"></open-data>  
        </view>
        <view class="flex-box p-r">
          <navigator hover-class="navigator-hover" url="../order/index?type=received"  open-type='navigate'>
            <text class="iconfont icon-daishouhuo orderListIcon">
              <!-- <text class='tag' wx:if='{{orderCount.received>0}}'>{{orderCount.received}}</text> -->
            </text>
            <text class="listCont">待收货</text>
            <text class="user-tag p-a" wx:if="{{orderCount.received > 0}}">{{orderCount.received}}</text>
          </navigator>
        </view>
        <view class="flex-box p-r">
          <navigator hover-class="navigator-hover" url="../order/index?type=complete"  open-type='navigate'>
            <text class="iconfont icon-daipingjia orderListIcon "></text>
            <text class="listCont">已完成</text>
            <text class="user-tag p-a" wx:if="{{false}}"></text>
          </navigator>
        </view>
      </view>
    </view>
  </view>
  <view class="m-top20">
    <navigator url="../address/index">
      <view class="user-order-title dis-flex b-f m-top20">
        <view class="flex-box">
          <text class="iconfont icon-map common-icon"></text>
          <text class="user-orderName h4">收货地址</text>
        </view>
        <view class="flex-box">
          <view class="user-orderContBox">
            <text class="iconfont icon-xiangyoujiantou user-orderJtou"></text>
          </view>
        </view>
      </view>
    </navigator>
    <navigator url="../user/help/help"  open-type='navigate'>
      <view class="user-order-title dis-flex b-f">
        <view class="flex-box">
          <text class="iconfont icon-help common-icon"></text>
          <text class="user-orderName h4">我的帮助</text>
        </view>
        <view class="flex-box">
          <view class="user-orderContBox">
            <text class="iconfont icon-xiangyoujiantou user-orderJtou"></text>
          </view>
        </view>
      </view>
    </navigator>
    <navigator url="../user/coupon/coupon"  open-type='navigate'>
      <view class="user-order-title dis-flex b-f">
        <view class="flex-box">
          <text class="iconfont icon-coupon common-icon"></text>
          <text class="user-orderName h4">我的优惠券</text>
        </view>
        <view class="flex-box">
          <view class="user-orderContBox">
            <text class="iconfont icon-xiangyoujiantou user-orderJtou"></text>
          </view>
        </view>
      </view>
    </navigator>
    <navigator url="../join/join"  open-type='navigate'>
      <view class="user-order-title dis-flex b-f">
        <view class="flex-box">
          <text class="iconfont icon-merchantJoin common-icon"></text>
          <text class="user-orderName h4">商户认证</text>
        </view>
        <view class="flex-box">
          <view class="user-orderContBox">
            <text class="iconfont icon-xiangyoujiantou user-orderJtou"></text>
          </view>
        </view>
      </view>
    </navigator>
    <navigator url="../user/share/share"  open-type='navigate' wx:if="{{userInfo.is_expend}}">
      <view class="user-order-title dis-flex b-f">
        <view class="flex-box">
          <text class="iconfont icon-fenxiao common-icon"></text>
          <text class="user-orderName h4">我要推广</text>
        </view>
        <view class="flex-box">
          <view class="user-orderContBox">
            <text class="iconfont icon-xiangyoujiantou user-orderJtou"></text>
          </view>
        </view>
      </view>
    </navigator>
  </view>
</view>
<tab-bar active="{{active}}" tab_bar="{{tab_bar}}"/>